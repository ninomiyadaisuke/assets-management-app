    "eslint-plugin-tailwindcss": "^3.13.0",
    "gh-pages": "^5.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "prisma": "^4.15.0",
    "storybook": "^7.1.0",
    "ts-node": "^10.9.1"
